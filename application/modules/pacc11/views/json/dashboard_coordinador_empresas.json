    "zones": [{
            "tiles": [{
                "title": "Buscar",
                "module": "pacc",
                "controller": "pacc",
                "function": "tile_buscar_container",
                "span": 6
            },
            {
                "title": "Asignar Evaluador",
                "module": "bpm",
                "controller": "kpi",
                "function": "tile",
                "span": 2,
                "params": ["pacc1PDE", "pacc11_PA_AE"]
            },
            {
                "title": "Recomendación al Comité",
                "module": "bpm",
                "controller": "kpi",
                "function": "tile",
                "span": 2,
                "params": ["pacc1PDE", "pacc1PDE_IJ"]
            },
            {
                "title": "Recomendación Aprobación-Rechazo",
                "module": "bpm",
                "controller": "kpi",
                "function": "tile",
                "span": 2,
                "params": ["pacc1PDEF", "pacc1PDEF_WR"]
            }


            ]
        }, {
            "full_widget": [{
                "title": "Full Table",
                "module": "pacc",
                "controller": "coordinador",
                "function": "tabla_prioridades"

            }]
        }, {
            "col1": [{
                "title": "Half Table",
                "module": "pacc",
                "controller": "coordinador",
                "function": "half_table_rank"
            }, {
                "title": "2do",
                "module": "pacc",
                "controller": "coordinador",
                "function": "knobs_empresas"

            }, {
                "title": "2do",
                "module": "pacc",
                "controller": "coordinador",
                "function": "knobs_emprendedores_pitchs"

            }, {
                "title": "2do",
                "module": "pacc",
                "controller": "coordinador",
                "function": "knobs_emprendedores"
            }]

        }, {
            "col2": [{
                "title": "Chart",
                "module": "charts",
                "controller": "coordinador",
                "function": "area_charts"
            }, {
                "title": "Map",
                "module": "pacc",
                "controller": "coordinador",
                "function": "map_demo"

            }]
        },

        {
            "2cols_after": [

            ]
        }
    ]
