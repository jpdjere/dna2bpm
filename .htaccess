<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteCond %{HTTP_HOST} ^www.accionpyme\.mecon\.gob\.ar$ [NC]
    RewriteRule ^(.*)$ http://dna2.produccion.gob.ar/dna2bpm/$1 [R=301,L]

    RewriteCond %{HTTP_HOST} ^www.accionpyme\.mecon\.gov\.ar$ [NC]
    RewriteRule ^(.*)$ http://dna2.produccion.gob.ar/dna2bpm/$1 [R=301,L]

    RewriteCond $1 !^(index\.php|images|API|robots\.txt|API|user_guide|dna2_user_guide|jscript|css)
